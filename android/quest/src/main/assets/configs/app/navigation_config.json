{
  "appId": "app",
  "configType": "navigation",
  "menuActionButton": {
    "id": "main_add_button",
    "visible": true,
    "display": "Add New Household",
    "actions": [
      {
        "type": "onClose",
        "workflow": "launch_profile",
        "id": "patient_profile"
      }
    ],
    "questionnaire": {
      "id": "82952",
      "title": "Add Family",
      "saveButtonText": "ADD FAMILY",
      "setPractitionerDetails": true,
      "setOrganizationDetails": true
    }
  },
  "staticMenu": [
    {
      "id": "reports",
      "visible": true,
      "display": "Reports"
    },
    {
      "id": "p2p_sync",
      "visible": true,
      "display": "Transfer Data"
    },
    {
      "id": "profile",
      "visible": true,
      "display": "Profile"
    }
  ],
  "clientRegisters": [
    {
      "id": "all_households",
      "display": "All Households",
      "icon": "",
      "showCount": true,
      "actions": [
        {
          "type": "onClick",
          "workflow": "launch_register",
          "id": "all_household_register"
        },
        {
          "type": "count",
          "rules": [
            {
              "name": "householdCount",
              "condition": "true",
              "action": [
                "householdCountValue = helper.count('all_household_register')"
              ]
            }
          ]
        }
      ]
    },
    {
      "id": "child",
      "display": "Child",
      "icon": "",
      "showCount": true
    },
    {
      "id": "sick",
      "display": "Sick",
      "icon": "",
      "showCount": true
    },
    {
      "id": "priority",
      "display": "Today's Priority",
      "icon": "",
      "showCount": true
    }
  ],
  "bottomSheetRegisters": {
    "visible": true,
    "display": "Other Patients",
    "registers": [
      {
        "id": "bottom_all_households",
        "display": "All Households",
        "showCount": true
      },
      {
        "id": "bottom_child",
        "display": "Child",
        "showCount": true
      },
      {
        "id": "bottom_sick",
        "display": "Sick",
        "showCount": true
      },
      {
        "id": "bottom_priority",
        "display": "Today's Priority",
        "showCount": true
      }
    ]
  }
}
