      "item": [
        {
          "linkId": "unable-on-scheduled-date-display",
          "text": "<b>Could not get to clinic on their scheduled appointment date</b>",
          "type": "display",
          "required": false
        },
        {
          "linkId": "unable-on-scheduled-date-reason",
          "text": "Why could they not get to the clinic on their scheduled appointment date?",
          "type": "choice",
          "required": true,
          "answerOption": [
            {
              "valueCoding": {
                "code": "work-interference",
                "display": "Unable to get off work or school"
              }
            },
            {
              "valueCoding": {
                "code": "other-priorities",
                "display": "Competing priorities - e.g business, working for difficult employers"
              }
            },
            {
              "valueCoding": {
                "code": "travel",
                "display": "Travel (in and out of the area or over holidays)"
              }
            },
            {
              "valueCoding": {
                "code": "relocation",
                "display": "Temporarily relocated"
              }
            },
            {
              "valueCoding": {
                "code": "no-transport-funds",
                "display": "Lack of transport funds"
              }
            }
          ]
        },
        {
          "linkId": "priorities-advice",
          "text": "<ol> <li>Attend appointment on possible day before scheduled date</li> <li>Weekend appointment</li> <li>Early morning or lunch hour clinic if available </li> <li>Call clinic to notify of missing appointment but has enough meds</li><li>Ask providers for next visit on best time of the week or month to set appointment</li> </ol> ",
          "type": "display",
          "required": false,
          "enableBehavior": "any",
